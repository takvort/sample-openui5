{
	"_version" : "1.1.0",

	"sap.app" : {
		"_version" : "1.1.0",
		"id" : "salesorders",
		"type" : "application",

		"i18n" : "i18n/message_bundle.properties",
		"title" : "{{appTitle}}",
		
		"applicationVersion" : {
			"version" : "1.0.0"
		}
	},

	"sap.ui" : {
		"_version" : "1.1.0",
		"technology" : "UI5",

		"deviceTypes" : {
			"desktop" : true,
			"tablet" : true,
			"phone" : true
		},

		"supportedThemes" : [
			"sap_hcb",
			"sap_bluecrystal",
			"sap_belize"
		]
	},

	"sap.ui5" : {
		"_version" : "1.1.0",

		"rootView" : {
			"id" : "appView",
			"viewName" : "salesorders.view.App",
			"type" : "XML"
		},

		"dependencies" : {
			"minUI5Version" : "1.30.3",
			"libs" : {
				"sap.m" : {}
			}
		},

		"models" : {
			"i18n" : {
				"type" : "sap.ui.model.resource.ResourceModel",
				"settings" : {
					"bundleName" : "salesorders.i18n.message_bundle"
				}
			}
		},

		"resources" : {
			"css" : [{
					"uri" : "css/style.css"
				}
			]
		},

		"routing" : {

			"config" : {
				"routerClass" : "sap.m.routing.Router",
				"viewType" : "XML",
				"viewPath" : "salesorders.view",
				"controlId" : "app",
				"controlAggregation" : "pages",
				"bypassed" : {
					"target" : "notFoundTarget"
				}
			},

			"routes" : [{
					"pattern" : "",
					"name" : "items",
					"target" : "itemsTarget"
				}, {
					"pattern" : "empty",
					"name" : "emptyDetail",
					"target" : "emptyTarget"
				}
			],

			"targets" : {
				"itemsTarget" : {
					"viewName" : "Items",
					"controlAggregation" : "pages",
					"transition" : "show"
				},
				"notFoundTarget" : {
					"viewName" : "NotFound",
					"transition" : "show"
				}
			}

		}

	}
}
